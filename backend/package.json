{
  "name": "supportcarr-backend",
  "version": "1.0.0",
  "description": "SupportCarr Platform - Backend API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "build:watch": "tsc --watch",
    "worker": "node dist/workers/index.js",
    "test": "NODE_ENV=test jest --runInBand",
    "test:unit": "NODE_ENV=test jest --testPathPattern=tests/unit --runInBand",
    "test:integration": "NODE_ENV=test jest --testPathPattern=tests/integration --runInBand",
    "test:e2e": "NODE_ENV=test jest --testPathPattern=tests/e2e --runInBand",
    "test:coverage": "NODE_ENV=test jest --coverage --runInBand",
    "test:watch": "NODE_ENV=test jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "db:seed": "ts-node -r tsconfig-paths/register src/scripts/seed.ts",
    "db:reset": "ts-node -r tsconfig-paths/register src/scripts/reset.ts"
  },
  "keywords": [
    "supportcarr",
    "rescue",
    "on-demand",
    "micromobility",
    "ebike"
  ],
  "author": "SupportCarr Team",
  "license": "UNLICENSED",
  "dependencies": {
    "@sentry/node": "^7.99.0",
    "@sentry/profiling-node": "^1.3.3",
    "aws-sdk": "^2.1550.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.9",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "mongoose-encryption": "^2.1.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.8",
    "prom-client": "^15.1.0",
    "socket.io": "^4.6.1",
    "socket.io-redis": "^6.1.1",
    "stripe": "^14.14.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^4.20.1",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss-clean": "^0.1.4",
    "zod": "^3.22.4",
    "module-alias": "^2.2.3",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/hpp": "^0.2.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.6",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
