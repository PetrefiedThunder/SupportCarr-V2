{
  "name": "supportcarr-backend",
  "version": "1.0.0",
  "description": "SupportCarr Platform - Backend API",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "worker": "node src/workers/index.js",
    "test": "NODE_ENV=test jest --runInBand",
    "test:unit": "NODE_ENV=test jest --testPathPattern=tests/unit --runInBand",
    "test:integration": "NODE_ENV=test jest --testPathPattern=tests/integration --runInBand",
    "test:e2e": "NODE_ENV=test jest --testPathPattern=tests/e2e --runInBand",
    "test:coverage": "NODE_ENV=test jest --coverage --runInBand",
    "test:watch": "NODE_ENV=test jest --watch",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "db:seed": "node src/scripts/seed.js",
    "db:reset": "node src/scripts/reset.js",
    "docs:generate": "node src/scripts/generateDocs.js"
  },
  "keywords": [
    "supportcarr",
    "rescue",
    "on-demand",
    "micromobility",
    "ebike"
  ],
  "author": "SupportCarr Team",
  "license": "UNLICENSED",
  "dependencies": {
    "@sentry/node": "^7.99.0",
    "@sentry/profiling-node": "^1.3.3",
    "apollo-server-express": "^3.13.0",
    "aws-sdk": "^2.1550.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.9",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^16.0.2",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "mongoose-encryption": "^2.1.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.8",
    "prom-client": "^15.1.0",
    "socket.io": "^4.6.1",
    "socket.io-redis": "^6.1.1",
    "stripe": "^14.14.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^4.20.1",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.6",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
